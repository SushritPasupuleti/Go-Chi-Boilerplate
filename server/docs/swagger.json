            "put": {
                "description": "Update User By Email",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "Update User By Email",
                "parameters": [
                    {
                        "type": "string",
